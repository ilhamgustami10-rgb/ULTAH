{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        
        { 
            "src": "/build/(.*)", 
            "dest": "/public/build/$1" 
        },
        
        { 
            "src": "/(.*\\.(?:css|js|jpe?g|png|gif|webp|svg|ico|mp4|webm|mp3|wav|ttf|woff2?|eot))$", 
            "dest": "/public/$1" 
        },
        
        { 
            "src": "/(.*)", 
            "dest": "/api/index.php",
            "headers": [
                {
                    "key": "X-Forwarded-Proto",
                    "value": "https"
                }
            ]
        }
    ],
    "public": true,
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://ultahsahfanilam.vercel.app", 
        "APP_KEY": "base64:TGaVaXwhWgjn9akhE4SCSIjGG/cQqvICUQ+PgLA3Bs0=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "4gg.h.filess.io",
        "DB_PORT": "3306",
        "DB_DATABASE": "ultah",
        "DB_USERNAME": "root"
    }
}